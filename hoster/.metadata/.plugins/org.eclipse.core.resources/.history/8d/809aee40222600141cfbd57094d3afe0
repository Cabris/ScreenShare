import java.nio.ByteBuffer;


public class main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ByteBuffer buffer = ByteBuffer.allocate(60);
		buffer.clear();
		byte[] data=new byte[45];
		for (int i = 0; i < data.length; i++) {
			data[i]=(byte) i;
		}
		buffer.put(data);
		
	}

}
